{
    "name": "railken/amethyst-api",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/bag": "^1.2",
        "railken/lem": "3.2.*",
        "league/fractal": "^0.17.0",
        "railken/lara-eye": "1.2.*",
        "railken/eloquent-mapper": "0.1.*",
        "fico7489/laravel-eloquent-join": "dev-ft-refactor"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.6.*",
        "railken/amethyst-foo": "dev-master"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/",
            "": "tests/laravel" 
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:railken/laravel-eloquent-join.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\ApiServiceProvider"
            ]
        }
    }
}
