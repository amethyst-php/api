{
    "name": "railken/amethyst-api",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/bag": "^1.2",
        "railken/lem": "3.2.*",
        "league/fractal": "^0.17.0",
        "railken/lara-eye": "1.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "phpstan/phpstan": "^0.10",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/",
            "": "tests/laravel" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\ApiServiceProvider"
            ]
        }
    }
}
